QWidget {
    background-color : background_dark;
    color : foreground_light;
}

QMenuBar:item:selected {
    background-color : background_dark;
    color : selection_light;
}

QMenu:item:selected {
    color : selection_light;
}

QMainWindow { 
    background-color : background_dark; 
}

QTreeView {
    border : 1px solid foreground_dark;
}

QTreeView::item {
    color : foreground_light;
}

QTreeView::item:hover {
    background-color : background_medium;
    color : selection_medium;
}

QTreeView::item:first:hover {
    background-color : background_medium;
    color : selection_medium;
    border : 1px solid foreground_dark;
    border-right : 0;
}

QTreeView::item:middle:hover {
    background-color : background_medium;
    color : selection_medium;
    border-top : 1px solid foreground_dark;
    border-bottom : 1px solid foreground_dark;
}

QTreeView::item:last:hover {
    background-color : background_medium;
    color : selection_medium;
    border : 1px solid foreground_dark;
    border-left : 0;
}

QTreeView::item:selected {
    background-color : background_light;
    color : selection_light;
}

QTreeView::item:selected:!active {
    color : foreground_light;
    background-color : background_light;
}

QTreeView::item QLineEdit {
    padding : 0 px;
    margin : 0 px;
    border : 0 px;
    color : black;
    selection-color : selection_medium;
    color : selection_medium;
    selection-background-color : selection_dark;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    image : url(:branch_closed.png);
}

QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover {
    image : url(:branch_closed_hi.png);
}

QTreeView::branch:has-children:!has-siblings:open,
QTreeView::branch:open:has-children:has-siblings {
    image : url(:branch_open.png);
}

QTreeView::branch:has-children:!has-siblings:open:hover,
QTreeView::branch:open:has-children:has-siblings:hover {
    image : url(:branch_open_hi.png);
}

QHeaderView::section {
    border : 1px solid foreground_dark;
    border-top : 0;
    border-right : 0;
    padding-left : 6px;
    background-color : background_medium;
    color : foreground_light;
}

QHeaderView::section:first {
    border-left : 0;
}

QTableWidget {
    border : 1px solid foreground_dark;
}

QTableWidget::item {
    selection-background-color : background_dark;
    color : foreground_light;
}

QPushButton { 
    border : 1px solid foreground_dark;
    padding : 1px 5px 1px 5px;
    background-color : background_medium;
    color : foreground_light;
}

QPushButton:disabled {
    background-color : background_medium;
    color : foreground_dark;
}

QPushButton:hover {
    color : selection_light;
    border : 1px solid foreground_medium;
    background-color : background_light;
}

QPushButton:pressed {
    background-color : background_medium;
}

QRadioButton::indicator {
    background-color : background_dark;
}

QRadioButton:disabled {
    color : foreground_dark;
}

QRadioButton:hover {
    color : selection_medium;
}

QRadioButton::indicator:unchecked {
    image : url(:radio-off.png);
}

QRadioButton::indicator:unchecked:disabled {
    image : url(:radio-off_lo.png);
}

QRadioButton::indicator:unchecked:hover {
    image : url(:radio-off_hi.png);
}

QRadioButton::indicator:checked {
    image : url(:radio-on.png);
}

QRadioButton::indicator:checked:disabled {
    image : url(:radio-on_lo.png);
}

QRadioButton::indicator:checked:hover {
    image : url(:radio-on_hi.png);
}

QCheckBox:disabled {
    color : foreground_dark;
}

QCheckBox:hover {
    color : selection_medium;
}

QCheckBox::indicator {
    background-color : background_dark;
}

QCheckBox::indicator:unchecked {
    image : url(:unchecked.png);
}

QCheckBox::indicator:unchecked:disabled {
    image : url(:unchecked_lo.png);
}

QCheckBox::indicator:unchecked:hover {
    image : url(:unchecked_hi.png);
}

QCheckBox::indicator:checked {
    image : url(:checked.png);
}

QCheckBox::indicator:checked:disabled {
    image : url(:checked_lo.png);
}

QCheckBox::indicator:checked:hover {
    image : url(:checked_hi.png);
}

QSpinBox {
    border : 1px solid foreground_dark;
    padding-right : 15px;
}

QSpinBox::up-button {
    subcontrol-origin : border;
    subcontrol-position : top right;
    background-color : background_medium;
    border : 1px solid foreground_dark;
    border-bottom : 0;
    image : url(:small_arrow_up.png);
    width : 15px;
}

QSpinBox::up-button:disabled {
    image : url(:small_arrow_up_lo.png);
}

QSpinBox::up-button:hover {
    image : url(:small_arrow_up_hi.png);
}

QSpinBox::up-button:pressed {
    image : url(:small_arrow_up.png);
}

QSpinBox::down-button {
    subcontrol-origin : border;
    subcontrol-position : bottom right;
    background-color : background_medium;
    border : 1px solid foreground_dark;
    border-top : 0;
    image : url(:branch_open.png);
    width : 15px;
}

QSpinBox::down-button:disabled {
    image : url(:branch_open_lo.png);
}

QSpinBox::down-button:hover {
    image : url(:branch_open_hi.png);
}

QSpinBox::down-button:pressed {
    image : url(:branch_open.png);
}

QListView {
    border : 1px solid foreground_dark;
    color : foreground_light;
}

QListView::item:hover {
    border : 1px solid foreground_dark;
    background-color : background_medium;
    color : selection_medium;
}

QListView::item:selected {
    background-color : background_light;
    color : selection_light;
}

QListView::item:selected:!active {
    color : foreground_light;
}

QListWidget {
    border : 1px solid foreground_dark;
}

QListWidget::item {
    color : foreground_light;
}

QListWidget::item:selected:!active {
    background-color : background_dark;
}

QListWidget::item:hover {
    color : selection_medium;
}

QListWidget::item:disabled {
    color : foreground_dark;
}

QListWidget::indicator:checked {
    image : url(:checked.png);
}

QListWidget::indicator:checked:disabled {
    image : url(:checked_lo.png);
}

QListWidget::indicator:unchecked {
    image : url(:unchecked.png);
}

QListWidget::indicator:unchecked:disabled {
    image : url(:unchecked_lo.png);
}

QComboBox {
    border : 1px solid foreground_dark;
}

QComboBox:disabled {
    color : foreground_dark;
}

QComboBox::drop-down {
    background-color : background_medium;
    border-left : 1px solid foreground_dark;
}

QComboBox::down-arrow {
    image : url(:branch_open.png);
}

QComboBox::down-arrow:disabled {
    image : url(:branch_open_lo.png);
}

QComboBox::down-arrow:hover {
    image : url(:branch_open_hi.png);
}

QComboBox::down-arrow:on {
    top : 1px;
    left : 1px;
    image : url(:small_arrow_up.png);
}

QScrollBar:vertical {
    border : 1px solid foreground_dark;
    border-right : 0;
    width : 18px;
    margin : 19px 0 19px 0;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background-color: background_light;
}

QScrollBar::handle:vertical {
    background-color : background_dark;
    padding-left : 1px;
    padding-right : 1px;
}

QScrollBar::sub-line:vertical:hover {
    image : url(:small_arrow_up_hi.png);
}

QScrollBar::sub-line:vertical:pressed {
    top : 1px;
    left : 1px;
    image : url(:small_arrow_up.png);
}

QScrollBar::add-line:vertical:hover {
    image : url(:branch_open_hi.png);
}

QScrollBar::add-line:vertical:pressed {
    top : 1px;
    left : 1px;
    image : url(:branch_open.png);
}

QScrollBar::add-line:vertical {
    background-color : background_dark;
    border : 0;
    border-left : 1px solid foreground_dark;
    height : 19px;
    image : url(:branch_open.png);
    subcontrol-position : bottom;
    subcontrol-origin : margin;
}

QScrollBar::sub-line:vertical {
    background-color : background_dark;
    border : 0;
    border-left : 1px solid foreground_dark;
    height : 19px;
    image : url(:small_arrow_up.png);
    subcontrol-position : top;
    subcontrol-origin : margin;
}

QScrollBar:horizontal {
    border : 1px solid foreground_dark;
    border-bottom : 0;
    height: 18px;
    margin: 0 19px 0 19px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background-color: background_light;
}

QScrollBar::handle:horizontal {
    background-color : background_dark;
    padding-left : 1px;
    padding-right : 1px;
}

QScrollBar::sub-line:horizontal:hover {
    image : url(:small_arrow_left_hi.png);
}

QScrollBar::sub-line:horizontal:pressed {
    top : 1px;
    left : 1px;
    image : url(:small_arrow_left.png);
}

QScrollBar::add-line:horizontal:hover {
    image : url(:branch_closed_hi.png);
}

QScrollBar::add-line:horizontal:pressed {
    top : 1px;
    left : 1px;
    image : url(:branch_closed.png);
}

QScrollBar::add-line:horizontal {
    background-color : background_dark;
    border : 0;
    border-top : 1px solid foreground_dark;
    width : 19px;
    image : url(:branch_closed.png);
    subcontrol-position : right;
    subcontrol-origin : margin;
}

QScrollBar::sub-line:horizontal {
    background-color : background_dark;
    border : 0;
    border-top : 1px solid foreground_dark;
    width : 19px;
    image : url(:small_arrow_left.png);
    subcontrol-position : left;
    subcontrol-origin : margin;
}

QSplitter {
    padding : 2px;
}

QTabWidget::pane {
    border : 1px solid foreground_dark;
}

QTabWidget::pane {
    position : absolute;
    top : -1px;
}

QTabBar::tab:top:hover {
    color : selection_light;
    background : background_light;
}

QTabBar::tab:top {
    background-color : background_medium;
    border : 1px solid foreground_dark;
    border-left : 0px;
    border-top-right-radius : 4px;
    border-top-left-radius : 4px;
    padding-top : 4px;
    padding-bottom : 4px;
    padding-left : 10px;
    padding-right : 10px;
}

QTabBar::tab:first {
    border-left : 1px solid foreground_dark;
}

QTabBar::tab:top:selected {
    background-color : background_dark;
    color : selection_medium;
    border-bottom : 0px;
}

QLabel:disabled {
    color : foreground_dark;
}

QLineEdit {
    border : 1px solid foreground_dark;
    selection-background-color : selection_dark;
}

QLineEdit:disabled {
    color : foreground_dark;
}

QLineEdit:hover:enabled {
    color : selection_medium;
}

QLineEdit:focus {
    color : selection_light;
    border : 1px solid foreground_medium;
}

QTextEdit {
    border : 1px solid foreground_dark;
}

QTextEdit:focus {
    border : 1px solid foreground_medium;
}

QGroupBox {
    border : 1px solid foreground_dark;
    margin-top : 3ex;
}

QGroupBox::title {
    subcontrol-origin : margin;
    left : 8px;
}

QGroupBox::title:disabled {
    color : foreground_dark;
}

QSlider::groove:horizontal {
    height : 4px;
    background : foreground_medium;
    border-radius : 2px;
}

QSlider::handle:horizontal {
  background : background_light;
  border : 1px solid background_medium;
  width : 8px;
  height : 4px;
  margin : -8px 0px;
  border-radius : 4px;
}

QSlider::groove:horizontal:disabled {
  background : background_medium;
}

